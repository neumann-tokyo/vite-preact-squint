{
	"name": "web",
	"dockerComposeFile": ["../docker-compose.yml"],
	"service": "web",
	"workspaceFolder": "/home/alice/web",
	"postAttachCommand": "git config --global core.pager \"less -FX\" && sudo chown -R alice:alice /home/alice/",
	"shutdownAction": "stopCompose",
	"initializeCommand": "ls",
	"containerUser": "alice",
	"customizations": {
		"vscode": {
			"extensions": [
				"wmaurer.change-case",
				"sleistner.vscode-fileutils",
				"usernamehw.errorlens",
				"albymor.increment-selection",
				"huizhou.githd",
				"victoriadrake.kabukicho",
				"naumovs.color-highlight",
				"betterthantomorrow.calva",
				"hoovercj.vscode-power-mode",
				"Mtuan.typing-sounds",
				"tonybaloney.vscode-pets",
				"klyap.vscode-celestial-magic-girl-icon-theme",
				"le-nn.vscode-vector-aquarium"
			],
			"settings": {
				"editor.formatOnSave": true,
				"editor.formatOnSaveMode": "file",
				"files.insertFinalNewline": true,
				"terminal.integrated.defaultProfile.linux": "bash",
				"terminal.integrated.profiles.linux": {
					"bash": { "path": "/bin/bash" }
				},
				"workbench.colorTheme": "Kabukich≈ç",
				"workbench.iconTheme": "vscode-celestial-magic-girl-icon-theme",
				"powermode.presets": "fireworks",
				"powermode.enabled": true,
				"powermode.combo.counterEnabled": "show",
				"powermode.combo.location": "editor",
				"powermode.combo.timerEnabled": "show",
				"powermode.shake.enabled": false,
				"vscode-pets.petColor": "yellow",
				"vscode-pets.petSize": "large",
				"vscode-pets.petType": "dog",
				"vscode-pets.position": "explorer",
				"vscode-pets.theme": "beach",
				"vscode-pets.throwBallWithMouse": true,
				"nyanMode.nyanDisable": false,
				"nyanMode.nyanDisplayPercent": true,
				"nyanMode.nyanAction": "moving",
				"nyanMode.nyanAnimation":"active",
				"nyanMode.nyanRainbowAnimation": true
			}
		}
	}
}
